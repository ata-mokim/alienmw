<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.alienasia.alienmw.dao.UserDAO"><!--namespace를 통해 UserDAO와 연결합니다. -->

    <select id="selectUsers" parameterType="com.alienasia.alienmw.dto.UserDTO"
            resultType="com.alienasia.alienmw.dto.UserDTO">
        SELECT user_id, password
        FROM USER_INFO
    </select>

</mapper>